<p id="notice"><%= notice %></p>

<h1>Listing Requests</h1>

<table>
  <thead>
    <tr>
      <th>Case</th>
      <th>Rdate</th>
      <th>Sent by</th>
      <th>Oriented</th>
      <th>Service</th>
      <th>Notes</th>
      <th>Qualification</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.case %></td>
        <td><%= request.rdate %></td>
        <td><%= request.sent_by %></td>
        <td><%= request.oriented %></td>
        <td><%= request.service %></td>
        <td><%= request.notes %></td>
        <td><%= request.qualification %></td>
        <td><%= request.person %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request', new_request_path %>
